{
  "name": "sliderx-react",
  "version": "0.0.5",
  "description": "Современный и гибкий React-компонент для создания слайдеров с поддержкой различных эффектов",
  "author": "Aidamir Kambiev",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Aidamirrrrrr/sliderx"
  },
  "keywords": [
    "react",
    "slider",
    "carousel",
    "swipe",
    "typescript",
    "animation"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src/**/*.{ts,tsx}",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0 || ^19.0.0",
    "react-dom": "^17.0.0 || ^18.0.0 || ^19.0.0",
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "classnames": "^2.5.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "prettier": "^3.2.5",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3",
    "typescript-plugin-css-modules": "^5.1.0"
  }
}
